  "keywords": [
    "farcaster",
    "mini-app",
    "game",
    "runner",
    "subway-surfer",
    "html5",
    "canvas",
    "javascript",
    "mobile",
    "touch",
    "vercel"
  ],
  "author": "Farcaster Developer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/farcaster-runner.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/farcaster-runner/issues"
  },
  "homepage": "https://github.com/yourusername/farcaster-runner#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "vercel": "^32.0.0"
  },
  "dependencies": {},
  "vercel": {
    "buildCommand": "npm run build",
    "outputDirectory": ".",
    "installCommand": "npm install",
    "framework": null
  }
