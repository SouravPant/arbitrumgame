
{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "engines": {
    "node": "18.x"
  },
  "functions": {
    "api/webhook.js": {
      "runtime": "@vercel/node@2.15.10"
    }
  },
  "env": {
    "VITE_CONTRACT_ADDRESS": "0xF3FF71531F523f09c42490760e9Ba43eD30bb0De",
    "VITE_ARBITRUM_CHAIN_ID": "421614",
    "VITE_NETWORK_NAME": "Arbitrum Sepolia",
    "VITE_RPC_URL": "https://sepolia-rollup.arbitrum.io/rpc",
    "VITE_EXPLORER_URL": "https://sepolia.arbiscan.io"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/.well-known/farcaster.json",
      "destination": "/.well-known/farcaster.json"
    }
  ]
}
